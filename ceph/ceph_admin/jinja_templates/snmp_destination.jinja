{% if "snmp_v3_auth_username" in spec["spec"]["credentials"] and "snmp_v3_auth_password" in spec["spec"]["credentials"] %}
  createuser -e 0x{{ spec["spec"]["engine_id"] }} {{ spec["spec"]["credentials"]["snmp_v3_auth_username"] }} SHA {{ spec["spec"]["credentials"]["snmp_v3_auth_password"] }}
  authuser log,execute {{ spec["spec"]["credentials"]["snmp_v3_auth_username"] }}
{% elif "snmp_community" in spec["spec"]["credentials"] %}
  authCommunity log,execute,net public
{% endif %}

format2 %V\n% Agent Address: %A \n Agent Hostname: %B \n Date: %H - %J - %K - %L - %M - %Y \n Enterprise OID: %N \n Trap Type: %W \n Trap Sub-Type: %q \n Community/Infosec Context: %P \n Uptime: %T \n Description: %W \n PDU Attribute/Value Pair Array:\n%v \n -------------- \n

